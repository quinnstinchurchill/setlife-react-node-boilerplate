{
    "presets": [
        ["@babel/env", {
            "modules": false
        }],
        "@babel/react"
    ],
    "plugins": [
        ["babel-plugin-styled-components", {
            "displayName": true
        }]
    ],
    "env": {
        "production": {
            "plugins": [
                ["transform-react-remove-prop-types", { "mode": "remove" }]
            ]
        }
    }
}