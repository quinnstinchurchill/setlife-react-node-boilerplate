{
  "name": "ui",
  "version": "0.0.0",
  "description": "React UI components used across all Setlife projects",
  "main": "build/index.js",
  "author": "Quinn Pruitt <qpruitt93@gmail.com>",
  "homepage": "https://github.com/quinnstonchurchill/setlife#readme",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quinnstonchurchill/setlife.git"
  },
  "keywords": [
    "components",
    "react",
    "ui",
    "design-system",
    "styled-system",
    "styled-components"
  ],
  "scripts": {
    "prepare": "yarn clean && yarn build",
